declare @mes int,@fecha int,@fechaAnterior int,@anio int,@NumDiaMes int, @NumDiaAnio int,@MesAnterior int
,@NumDiaLab int,@NumAnioLab int,@totalHoy float,@totalMes float,@totalAnio float,@DiaProm int

select 
@fecha = feidfecha, 
@mes = fecalmes,
@anio = fecalanio ,
@MesAnterior = feMesAntCierr,
@fechaAnterior = fecIdAnterior,
@NumDiaLab = feNumDiasMesFiscalLab,
@NumAnioLab = feNumDiasAnioFiscalLab,
@DiaProm = feValorCalculoPromedio
from DWDFecha 
where feDiaActual = 1


select
@NumDiaMes = sum(isnull(case when fecalmes = @mes then feValorCalculoPromedio end,0)) ,
@NumDiaAnio = sum(isnull(case when feCalAnio = @anio then feValorCalculoPromedio end,0)) 
from dwdfecha where feCalAnio = @anio and feidfecha &lt;= @fecha



select distinct
indicadorNivel1,
indicadorNivel2,
indicadorNivel3,
ProductoNivel1= case when indicadorNivel1 = 2 then 'OBLIGACIONES CON EL PUBLICO' else ProductoNivel1 end,
ProductoNivel2= case when indicadorNivel2=142 then 'Depósitos a '+ProductoNivel2 else 'Depósitos a la vista' end,
ProductoNivel3='Depósitos '+ProductoNivel2,
ProductoNivel4=case when  indicadorNivel2 in (140,141) and indicadorNivel3 &lt; 250 then ccSubGrupo else case when indicadorNivel3 in (600,601) then 'Ahorros' else '' end end,
CpEstadoProducto
into #vuClasificacionProductos
from vuClasificacionProductos
inner join #DWDCuentaContable on ccCodigoIndicador = indicadorNivel3

update cc
set
cccodigoindicador = 52
from #DWDCuentaContable cc
where ccCodigoNivel4 = '41013580'

update cc
set
cccodigoindicador = 53
from #DWDCuentaContable cc
where ccCodigoNivel2 in ('4103','4104')

select 
@fecha bsidfecha,
ProductoNivel1,
ProductoNivel2,
ProductoNivel3,
ProductoNivel4,
caAgrupacion,
case when indicadorNivel2 = 141 then pocodtasa else '' end ProductoNivel5,
0 diapart,
sum(isnull(poSaldoOperacionUSD,0)) / 1000 SaldoHoy,
isnull(case when sum(poSaldoOperacionUSD) = 0 then 0 else sum(poSaldoOperacionUSD * potasanominal) / sum(poSaldoOperacionUSD) end,0) tasaDia,
(sum(isnull(poSaldoOperacionUSD * potasanominal,0)) /365)/ 100000 PYGDia,
0 mesPart,
sum(isnull(poSaldoPromedioMensualFiscal,0)) / 1000 saldomesF,
isnull(case when sum(poSaldoPromedioMensualFiscal) = 0 then 0 else (sum(poIntPromedioMensualFiscal) / sum(poSaldoPromedioMensualFiscal)) end,0) tasaMes,
(sum(isnull(poIntPromedioMensualFiscal,0)/100)/365)/1000 * @NumDiaMes PYGMes,
0 Aniopart,
sum(isnull(poSaldoPromedioAnualFiscal,0)) / 1000 SaldoAnioF,
isnull(case when sum(poSaldoPromedioAnualFiscal) = 0 then 0 else (sum(poIntPromedioAnualFiscal) / sum(poSaldoPromedioAnualFiscal)) end,0) tasaAnio,
(sum(isnull(poIntPromedioAnualFiscal,0)/100)/365)/1000 * @NumDiaAnio PYGAnio,
@NumDiaMes NumDiasMes,
@NumDiaAnio NumDiasAnio
into #TMP
from DWHPasivoOperacion
inner join DWDCuentaContable on poIdCuentaContable = ccIdCuentaContable
inner join #vuClasificacionProductos on ccCodigoIndicador = indicadorNivel3
inner join DWDFecha on poIdFecha = feIdFecha
inner join DWDCliente on poIdCliente = clIdCliente
inner join DWDCanal on clIdCanal = caIdCanal
where indicadorNivel2 in (140,141,142)
and abs(poSaldoOperacionUSD+poSaldoPromedioMensualFiscal+poSaldoPromedioAnualFiscal) &gt; 0
AND POIDFECHA = @fecha 
group by  ProductoNivel1,
ProductoNivel2,
ProductoNivel3,
ProductoNivel4,
caAgrupacion,
case when indicadorNivel2 = 141 then pocodtasa else '' end

union

