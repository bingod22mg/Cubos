when sc.scIdCodigo in (1,2,3) then sc.scSegmentacionTipo
else sc.scSegmentacion
end,
Nivel4=
case
when sc.scIdCodigo in (1,2,3) then sc.scSegmentacionTipo
else sc.scDetalleNombre
end,
